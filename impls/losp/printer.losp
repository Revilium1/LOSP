from losptypes import LospSymbol
def pr_str(losp_obj):
    if isinstance(losp_obj, LospSymbol):
        return losp_obj.name
    
    elif isinstance(losp_obj, (int, float)):
        return str(losp_obj)
    
    elif isinstance(losp_obj, list):
        inner = ' '.join(pr_str(el) for el in losp_obj)
        return f'({inner})'
    
    else:
        raise TypeError(f"Cannot print object of type {type(losp_obj)}")