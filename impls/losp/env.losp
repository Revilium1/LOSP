class Env:
    def __init__(self, outer=None):
        self.outer = outer
        self.data = {}

    def set(self, key, value):
        self.data[key] = value

    def get(self, key):
        if key in self.data:
            return self.data[key]
        elif self.outer is not None:
            return self.outer.get(key)
        else:
            # IMPORTANT: use the symbol name
            raise KeyError(f"{key.name} not found")
